n    --back_blue: #dfdfdf0f;\n  }\n\nhtml {\n    height: 100%;\n  }\n\nbody {\n    background-color: var(--gray_blue);\n    font-family: Inter;\n    height: 100%;\n  }\n\n\n/* Text Styles */\n.headline_1, .headline_2 {\n    color: var(--white);\n    font-weight: 600;\n    text-align: left;\n    -webkit-text-size-adjust: 100%;\n  }\n\n.headline_1 {\n    font-size: 20px;\n    margin-bottom: -4px;\n    }\n\n.content {\n    max-width: initial;\n    font-size: 14px;\n  }\n\n.content.bold {\n    font-weight: 500;\n  }\n\n.content.light {\n    font-weight: 400;\n  }\n\n.content.primary {\n    color: var(--white);\n  }\n\n.content.secondary {\n    color: var(--light-white);\n  }\n\n.content.butt.secondary:hover {\n    color: var(--white);\n  }\n\n.active_text {\n    color: var(--green)!important;\n}\n\n\n/* Images */\n.pic {\n    aspect-ratio: 1;\n  }\n\n.person_pic {\n    width: 22px;\n  }\n\n.study_pic {\n    width: 40px;\n  }\n\n\n/* Buttons */\n.base_button {\n    width: fit-content;\n    align-items: center;\n    text-align: center;\n    display: flex;\n}\n\n.base_button:hover {\n    background-color: rgba(255, 255, 255, 0.08);\n}\n\n.base_button.round {\n    gap: 5px;\n    padding: 7px 12px 7px 9px; \n    border-radius: 8px;\n}\n\n.base_button.confirm {\n    gap: 5px;\n    padding: 9px 12px; \n    border-radius: 8px;\n    background-color: var(--bluish_blue);\n    color: var(--white);\n}\n\n.base_button.confirm:hover {\n    background-image: linear-gradient(rgb(255 255 255/10%) 0 0);\n}\n\n.base_button.square {\n    width: inherit;\n    gap: 7px;\n    padding: 12px 12px 12px 9px;\n}\n\n.base_button.square.chosen {\n    background-color: var(--dark_blue);\n}\n\n.base_button.square.available {\n    background-color: none;\n}\n\n.base_button.remove {\n    padding: 7px 12px; \n    border-radius: 8px;\n    color: var(--red);\n}\n\n\n/* Containers */\n.upper_container {\n    flex-direction: row;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    flex-wrap: nowrap;\n\n    min-height: 51.5px;\n    padding: 7.5px 16px;\n    border-bottom: solid 1.5px var(--up_blue);\n  }\n\n.upper_container div {\n    flex-direction: row;\n    display: flex;\n    align-items: center;\n    gap: 8px;\n}\n\n.left_menu {\n    width: 285px;\n    height: 100%;\n    border-right: solid 1.5px var(--up_blue);\n}\n\n.left_menu div {\n    width: inherit;\n}\n\n.cells_container {\n    flex-direction: column;\n}\n\n.main_container {\n    height: 100%;\n    flex-direction: row;\n    display: flex;\n}\n\n.page_content {\n    margin: 26px;\n    gap: 32px;\n    flex-direction: column;\n    display: flex;\n}\n\n.content_section {\n    gap: 18px;\n    flex-direction: column;\n    display: flex;\n}\n\n.content_section.small {\n    gap: 16px;\n}\n\n.content_section.popup {\n    gap: 16px;\n    margin-top: 10px;\n}\n\n.content.checkbox {\n    padding-left: 6px;\n}\n\n.person_container {\n    gap: 10px;\n    flex-direction: row;\n    display: flex;\n    align-items: center;\n}\n\n/* Checkbox */\ninput[type=\"checkbox\"] {\n    width: 16px;\n    height: 16px;\n    -webkit-appearance: none;\n    -moz-appearance: none;\n    -o-appearance: none;\n    appearance:none;\n\n    border-radius: 5px;\n    border: 1.5px solid var(--up_blue);\n    outline: none;\n    background-color: none;\n}\n\ninput[type=\"checkbox\"]:checked {\n    background-color: var(--white);\n    border: 1.5px solid var(--white);\n}\n\n.check_temlp {\n    margin-top: 2px;\n    margin-bottom: -12px;\n}\n\n.check_temlp li {\n    margin-bottom: 16px;\n    display: flex;\n    flex-direction: row;\n    place-items: center;\n    gap: 6px;\n}\n\n.checkbox_custom:checked + .content {\n    font-weight: 500;\n    color: var(--white);\n  }\n\n/* Popup styles */\n.popup_back {\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0,0,0,0.75);\n    overflow: hidden;\n    position: fixed;\n    top: 0px;\n    display: none; \n    align-items: center;\n    justify-content: center; \n}\n\n.popup_container {\n    min-width: 405px;\n    gap: 18px;\n    padding: 26px;\n    border-radius: 12px;\n    background-color: var(--gray_blue);\n    box-shadow: 0px 0px 30px var(--back_blue);\n}\n\n/* Text input */\n.input_section {\n    gap: 8px;\n    flex-direction: column;\n    display: flex;\n}\n\n.text_input {\n    background-color: #f3f3f324;\n    border-width: 1px;\n    border: solid #ffffff4a 1px;\n    border-radius: 8px;\n    padding: 8px;\n}\n\n.ta_input {\n    background-color: #f3f3f324;\n    border-width: 1px;\n    border: solid #ffffff4a 1px;\n    border-radius: 8px;\n    padding: 8px;  \n}\n\n::placeholder {\n    color: var(--light-white);\n    opacity: 0.75;\n}\n\n/* Buttons section */\n.buttons_section {\n    margin-top: 8px;\n    flex-direction: row;\n    display: flex;\n    gap: 8px;\n}"},"lineCount":301}},"error":null,"hash":"a99f09ee6c688188df60de7fbac32ee7","cacheData":{"env":{}}}